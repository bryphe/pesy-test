{
  "name": "pesy-test",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "PesyTestApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["pesy-test.lib"],
      "main": "TestPesyTest",
      "name": "TestPesyTest.exe"
    },
    "library": {
      "name": "pesy-test.lib",
      "namespace": "PesyTest"
    },
    "executable": {
      "require": ["pesy-test.lib"],
      "main": "PesyTestApp",
      "name": "PesyTestApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestPesyTest.exe"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
